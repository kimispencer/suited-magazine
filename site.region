<!doctype html>
<html>
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<meta name="viewport" content="initial-scale=1">

		<!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
		{squarespace-headers}
	</head>

	<body>

	<div class="container">

		<header class="header">
			{.section website}
			<h1 class="site-title"><a href="/">{siteTitle}</a></h1>
			{.end}
		</header>

		<!-- navigation hook-up
			* template: /blocks/navigation.block
			* navigationId -> comes from template.conf, pages are added to nav on Squarespace admin
		-->
		<squarespace:navigation navigationId="mainNav" template="navigation" />
		
		<main class="content">
			<section id="page" role="main" data-content-field="main-content">
				{squarespace.main-content}
			</section>
		</main>

	</div>

	<!--Injection point for tracking scripts and user content from the code injection tab.-->
	{squarespace-footers}	
	</body>
</html>